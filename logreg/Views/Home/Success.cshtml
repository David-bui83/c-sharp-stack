@{
    ViewData["Title"] = "Success";
}
<a class="float-right" href="/logout">Logout</a>
<h1 class="text-center">@ViewData["Title"]</h1>

<div class="mt-5">
    <table class="table table-dark text-success">
        <thead>
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Password</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @{
                foreach(var user in @Model)
                {
                    <tr>
                        <td>@user.UserId</td>
                        <td>@user.FirstName</td>
                        <td>@user.LastName</td>
                        <td>@user.Email</td>
                        <td>@user.Password</td>
                        <td>
                            <a href="/delete/@user.UserId">Delete</a>
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
